<?xml version="1.0" encoding="UTF-8"?>
<builder>
    <consts
        overwrite="true"
        db_url="jdbc:mysql://127.0.0.1:3306/upsss?useUnicode=true&amp;characterEncoding=UTF-8"
        db_user="root"
        db_password=""
        mould_root="D:/workspace/builder/mould/1/"
        src_out="D:/workspace/builder_demo/src/com/unblocked/upsss/"
        test_out="D:/workspace/builder_demo/test/com/unblocked/upsss/"
        js_out="D:/workspace/builder_demo/WebRoot/source/view_js/"
        jsp_out="D:/workspace/builder_demo/WebRoot/WEB-INF/view_jsp_back/" />

    <entities>
        <entity
            table="upsss_area"
            title="区域"
            url="area"
            type="Area" />
        <entity
            table="upsss_button"
            title="按钮"
            url="button"
            type="Button" />
        <entity
            table="upsss_client"
            title="联系人"
            url="client"
            type="Client" />
        <entity
            table="upsss_menu"
            title="菜单"
            url="menu"
            type="Menu" />
        <entity
            table="upsss_pro_production"
            title="商品"
            url="production"
            type="Production" />
        <entity
            table="upsss_pro_productionkind"
            title="商品分类"
            url="production_kind"
            type="ProductionKind" />
        <entity
            table="upsss_user_role"
            title="角色"
            url="role"
            type="Role" />
        <entity
            table="upsss_transaccount_history"
            title="转账历史"
            url="transaccount_history"
            type="TransaccountHistory" />
        <entity
            table="upsss_transaccount_order"
            title="转账订单"
            url="transaccount_order"
            type="TransaccountOrder" />
        <entity
            table="upsss_transaccount_orderdetail"
            title="转账订单详情"
            url="transaccount_orderdetail"
            type="TransaccountOrderdetail" />
        <entity
            table="upsss_accountkind"
            title="账户类型"
            url="account_kind"
            type="AccountKind" />
        <entity
            table="upsss_bank_account"
            title="银行账户"
            url="bank_account"
            type="BankAccount" />
        <entity
            table="upsss_instock_order"
            title="入库订单/开单"
            url="instock_order"
            type="InstockOrder" />
        <entity
            table="upsss_shop"
            title="店铺"
            url="shop"
            type="Shop" />
        <entity
            table="upsss_stock_location"
            title="库位"
            url="stock_location"
            type="StockLocation" />
    </entities>

    <moulds>
        <mould
            name="${mould_root}/Action.java.ftl"
            out="${src_out}/action/${entity.type}Action.java" />
        <mould
            name="${mould_root}/IService.java.ftl"
            out="${src_out}/service/I${entity.type}Service.java" />
        <mould
            name="${mould_root}/ServiceImpl.java.ftl"
            out="${src_out}/service/impl/${entity.type}ServiceImpl.java" />
        <mould
            name="${mould_root}/IDao.java.ftl"
            out="${src_out}/dao/I${entity.type}Dao.java" />
        <mould
            name="${mould_root}/DaoImpl.java.ftl"
            out="${src_out}/dao/impl/${entity.type}DaoImpl.java" />
        <mould
            name="${mould_root}/DaoTest.java.ftl"
            out="${test_out}/dao/${entity.type}DaoTest.java" />
        <mould
            name="${mould_root}/ServiceTest.java.ftl"
            out="${test_out}/service/${entity.type}ServiceTest.java" />
        <mould
            name="${mould_root}/Model.java.ftl"
            out="${src_out}/model/Upsss${entity.type}.java" />
        <mould
            name="${mould_root}/Model.hbm.xml.ftl"
            out="${src_out}/model/hbm/${entity.type}.hbm.xml" />
        <mould
            name="${mould_root}/list.js.ftl"
            out="${js_out}/${entity.url}/list.js" />
        <mould
            name="${mould_root}/list.jsp.ftl"
            out="${jsp_out}/${entity.url}/list.jsp" />
        <mould
            name="${mould_root}/edit.jsp.ftl"
            out="${jsp_out}/${entity.url}/edit.jsp" />
    </moulds>
</builder>